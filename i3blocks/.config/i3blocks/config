separator_block_width=15

[volume]
command=~/.local/bin/volume
interval=once
markup=pango
signal=10

[battery]
command=echo "$(cat /sys/class/power_supply/BAT0/capacity)"
interval=30
markup=pango

# Update time every 1 seconds
[time]
command=~/.local/bin/time
interval=1
# color=#F0C674
# markup=pango
